{
    "name": "yangify",
    "version" : "1.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "description": "Replace 'Andrew Yang' with 'Yang Gang'",
    "icons": {
        "16": "yang_gang_16.png",
        "48": "yang_gang_48.png",
        "128": "yang_gang_128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Yangify"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
}
    
